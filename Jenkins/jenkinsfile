pipeline
{
	agent any 
	

	stages
	{
		stage('Log the Jenkins Docker Ant Git and Java version info')
		{
			steps 
			{
				sh 'java -version'
				sh 'git --version'
			}
		}
		stage('Run python Script')
		{
			steps
			{
				sh 'python -m ensurepip install -r requirements.txt'
				sh 'python main.py'
			}
		}
	}
}
