pipeline
{
	agent any 
	

	stages
	{ 
		stage('Build')
		{
			steps 
			{
				sh "echo 123"
			}
		}
		stage('Log the Jenkins Docker Ant Git and Java version info')
		{
			steps 
			{
				sh 'java -version'
				sh 'git --version'
			}
		}
		stage('Run python Script')
		{
			steps
			{
				sh 'virtualenv venv && . venv/bin/activate && pip install -r requirements.txt && python main.py'
			}
		}
	}
}
