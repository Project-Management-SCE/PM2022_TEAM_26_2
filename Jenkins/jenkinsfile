pipeline {
        agent none
         stages {
            stage('Build') {
                agent {
                    docker { 
						image 'androidsdk/android-31:latest'
						reuseNode true  
					}
						steps {
							sh 'java -version'
							sh 'git --version'
						}
					}
	    }
	}
}
		
		
		
		
