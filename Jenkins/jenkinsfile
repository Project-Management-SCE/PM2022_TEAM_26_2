pipeline
{
	agent any
	

	stages
	{
		stage('Log the Jenkins Docker Ant Git and Java version info')
		{
			steps 
			{
				sh 'java -version'
				sh 'git --version'
			}
		}

		stage('Build python') 
		{
			steps
			{
				sh 'python -m pip install -r requirements.txt'
				sh 'python main.py'
			}
		}
	}
}
				



		
